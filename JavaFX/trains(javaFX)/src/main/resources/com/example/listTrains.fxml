<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox fx:controller="com.example.ListTrainsController" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Наименование поезда: "/>
    <Label fx:id="trainNameLabel"/>
    <Label text="Тип поезда: "/>
    <Label fx:id="trainTypeLabel"/>
    <Label text="Количество вагонов: "/>
    <Label fx:id="trainCarCountLabel"/>
    <Label text="Количество мест в вагоне: "/>
    <Label fx:id="trainSeatsCountLabel"/>
    <Button id="backBtn" fx:id="backBtn" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="132.0" text="Назад" />

</VBox>
